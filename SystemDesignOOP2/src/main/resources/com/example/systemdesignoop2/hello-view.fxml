<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="254.0" prefWidth="236.0" spacing="20.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.systemdesignoop2.HelloController">
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <VBox prefHeight="200.0" prefWidth="100.0">
            <HBox.margin>
               <Insets left="50.0" right="50.0" />
            </HBox.margin>
            <children>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text=" Welcome Manager, where would you like to go?" textAlignment="CENTER" wrappingWidth="108.92302703857422">
                  <VBox.margin>
                     <Insets left="28.0" right="20.0" top="30.0" />
                  </VBox.margin>
               </Text>
            </children>
            <padding>
               <Insets right="10.0" />
            </padding>
         </VBox>
      </children>
   </HBox>
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <VBox prefHeight="170.0" prefWidth="132.0">
            <HBox.margin>
               <Insets left="65.0" />
            </HBox.margin>
            <children>
               <Button fx:id="movieListButton" mnemonicParsing="false" onAction="#onMovieListButtonClick" prefHeight="26.0" prefWidth="94.0" text="Movie List">
                  <VBox.margin>
                     <Insets bottom="10.0" left="20.0" />
                  </VBox.margin>
               </Button>
               <Button fx:id="screeningListButton" mnemonicParsing="false" onAction="#onScreeningListButtonClick" prefHeight="26.0" prefWidth="93.0" text="Screening List">
                  <VBox.margin>
                     <Insets bottom="10.0" left="20.0" />
                  </VBox.margin>
               </Button>
               <Button fx:id="showtimesListButton" mnemonicParsing="false" onAction="#onShowtimesListButtonClick" text="Showtimes List">
                  <VBox.margin>
                     <Insets bottom="10.0" left="20.0" />
                  </VBox.margin>
               </Button>
               <Button fx:id="ticketSalesButton" mnemonicParsing="false" onAction="#onTicketSalesButtonClick" prefHeight="26.0" prefWidth="96.0" text="Ticket Sales">
                  <VBox.margin>
                     <Insets left="20.0" />
                  </VBox.margin>
               </Button>
               <Button fx:id="clientListButton" mnemonicParsing="false" onAction="#onClientListButtonClick" prefHeight="26.0" prefWidth="95.0" text="Client List">
                  <VBox.margin>
                     <Insets left="20.0" top="10.0" />
                  </VBox.margin>
               </Button>
            </children>
         </VBox>
      </children>
   </HBox>
   <padding>
      <Insets bottom="20.0" />
   </padding>
</VBox>
